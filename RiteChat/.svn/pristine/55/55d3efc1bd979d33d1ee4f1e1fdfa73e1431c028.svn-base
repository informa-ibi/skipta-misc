<?php

class IsUserBlocked
{

  /**
   * 
   * @var ClientSkiptaSession $session
   * @access public
   */
  public $session;

  /**
   * 
   * @var guid $friendToCheck
   * @access public
   */
  public $friendToCheck;

  /**
   * 
   * @param ClientSkiptaSession $session
   * @param guid $friendToCheck
   * @access public
   */
  public function __construct($session, $friendToCheck)
  {
    $this->session = $session;
    $this->friendToCheck = $friendToCheck;
  }

}
