<?php

class SkiptaInboxMessagesResponse
{

  /**
   * 
   * @var array $Messages
   * @access public
   */
  public $Messages;

  /**
   * 
   * @var int $UnreadMessageCount
   * @access public
   */
  public $UnreadMessageCount;

  /**
   * 
   * @var int $MessageCount
   * @access public
   */
  public $MessageCount;

  /**
   * 
   * @var boolean $HasUnreadMessages
   * @access public
   */
  public $HasUnreadMessages;

  /**
   * 
   * @param array $Messages
   * @param int $UnreadMessageCount
   * @param int $MessageCount
   * @param boolean $HasUnreadMessages
   * @access public
   */
  public function __construct($Messages, $UnreadMessageCount, $MessageCount, $HasUnreadMessages)
  {
    $this->Messages = $Messages;
    $this->UnreadMessageCount = $UnreadMessageCount;
    $this->MessageCount = $MessageCount;
    $this->HasUnreadMessages = $HasUnreadMessages;
  }

}
